{
  "expo": {
    "name": "Octane",
    "scheme": "octane",
    "slug": "octane",
    "version": "1.0.0",
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": [
      "expo-router",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends.",
          "cameraPermission": "The app needs access to your camera to let you take photos."
        }
      ]
    ],
    "orientation": "portrait",
    "icon": "./assets/icon.jpg",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/playstore.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/newIcon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.mostafa_ahmed.octane",
      "softwareKeyboardLayoutMode": "pan"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "f94e9c1d-e418-4dcb-8eac-58c167d6131a"
      }
    }
  }
}
